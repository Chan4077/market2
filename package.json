	"scripts": {
		"start": "ng serve",
		"lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
		"test": "ng test",
		"pree2e": "webdriver-manager update --standalone false --gecko false",
		"e2e": "protractor",
		"firebaseDeploy": "ng build -prod && firebase deploy && rm -rf dist/",
		"checkNewDeps": "ncu -u",
		"updateDeps": "npm update",
		"forceUpdateDeps": "ncu -a && npm install",
		"postinstall": "npm i @angular/flex-layout@latest"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^4.4.4",
